{
    "name": "OS Development",
    "image": "mcr.microsoft.com/devcontainers/cpp",
    "features": {
      "ghcr.io/devcontainers/features/gcc": "latest",
      "ghcr.io/devcontainers/features/cmake": "latest"
    },
    "postCreateCommand": "sudo apt update && sudo apt install -y qemu-system-x86 nasm make",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-vscode.cpptools",
          "twxs.cmake",
          "ms-vscode.makefile-tools"
        ]
      }
    }
  }
  